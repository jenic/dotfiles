! X Cursor
Xcursor.theme: locolor

! URxvt ignores fontconfig?
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 0
Xft.rgba: rgb

!URxvt*termName: rxvt-256color
URxvt*title: Terminal
URxvt*saveLines: 512
URxvt*modifier: alt
URxvt*tripleclickwords: true
URxvt*urgentOnBell: true
URxvt*mapAlert: true

!URxvt*transparent: true
URxvt.depth: 32
URxvt.fading: 25
!URxvt*shading: 10
!URxvt*fading: 25
URxvt*background: rgba:0000/0000/0000/aaaa
URxvt*font: xft:Terminus:pixelsize=18:antialias=false:hinting=true
URxvt*boldFont: xft:Terminus:bold:pixelsize=18:antialias=false:hinting=true
URxvt*scrollBar: false
URxvt*scrollstyle: rxvt

URxvt*perl-ext-common: default,confirm-paste,url-select
URxvt.keysym.M-u:    perl:url-select:select_next
URxvt.url-select.launcher:   /usr/bin/firefox -new-tab
URxvt.url-select.underline: true
URxvt*urlLauncher: firefox
URxvt*matcher.button: 1
!URxvt*keysym.M-u: perl:url-select:select_next
!URxvt*url-select.underline: true

! omfg STAHP
URxvt.iso14755: false

! Be Fabulous
URxvt*colorUL: #86a2be
URxvt*foreground: White
!URxvt*background: Black


